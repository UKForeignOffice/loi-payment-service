    <script>

        function startTimer(timerDuration) {
            var timeoutDurationInSeconds = timerDuration / 1000;
            var display = document.querySelector('#displayTime');
            var timer = timeoutDurationInSeconds,
                minutes,
                seconds;
            set_visible('expiry-warning');
            var interval = setInterval(function(){
                minutes = parseInt(timer / 60, 10);
                seconds = parseInt(timer % 60, 10);
                display.textContent =
                    minutes + ' minute(s) and ' + seconds + ' seconds';
                timer = timer - 1;
                if (timer < 0) {
                    clearInterval(interval);
                    set_invisible('expiry-warning');
                    window.location.href = '/api/payment/session-expired';
                }
            }, 1000);
        }

        var sessionTimeoutWarning = 300000; // Show warning x milliseconds before session expiry
        var sessionTimeoutDisplay = 1500000; //Show popup after x milliseconds of inactivity
        setTimeout(function () {
            var timerDuration = sessionTimeoutWarning;
            startTimer(timerDuration)
        }, sessionTimeoutDisplay);

        function set_visible(id) {
            var e = document.getElementById(id);
            e.style.display = 'block';
        }

        function set_invisible(id) {
            var e = document.getElementById(id);
            e.style.display = 'none';
        }
    </script>

    <div id="expiry-warning" class="modal-container" role="dialog" aria-live="polite"
         aria-labelledby="dialog-title" aria-describedby="displayTime">
        <div class="modal-content-area">
            <h1 id="dialog-title" class="heading-large">
                Your application will time out soon
            </h1>
            <p class="govuk-body">We will reset your application if you do not respond in <span id="displayTime"></span></p>
            <div class="session-expiry-popup__clock" aria-hidden="true" aria-relevant="additions">
                <img class='countdown-clock' src='/api/payment/images/icon-countdown-clock.png' alt='Countdown clock' />
            </div>
            <div class="govuk-body session-expiry-popup__countdown" role="status"></div>
            <p class="govuk-body">We do this to keep your information secure.</p>
            <p>
                <button onclick="window.location.reload();" class="govuk-button govuk-button--secondary">Continue application</button>
            </p>
        </div>
    </div>
